    "camelcase": "^2.1.0",
    "choo": "^3.2.2",
    "common-errors": "^1.0.0",
    "component-closest": "^1.0.0",
    "compression": "^1.6.2",
    "connect-pg-simple": "^3.1.0",
    "cookie-parser": "^1.4.0",
    "dotenv": "^1.2.0",
    "envify": "^3.4.0",
    "errorhandler": "^1.4.3",
    "express": "^4.13.3",
    "express-jwt": "^3.4.0",
    "express-session": "^1.13.0",
    "failure": "^1.1.1",
    "form-serialize": "^0.7.1",
    "hint": "github:kvnneff/hint",
    "joi": "^7.0.1",
