{
  "name": "egg-finder",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "5.4.0"
  },
  "scripts": {
    "test": "npm run test-server && npm run test-client",
    "test-server": "NODE_ENV=test mocha src/test/server/index.js",
    "test-client": "zuul -- ./src/test/client/**/*.js",
    "test-client-local": "zuul --local -- ./src/test/client/**/*.js",
    "start": "node src/server",
    "start-dev": "supervisor -e js,html -w ./src/server -- src/server/",
    "watch": "watchy -w src/client/ -- npm run build-client",
    "build-client": "npm run build-css && npm run build-js",
    "build-css": "postcss -u postcss-import -u postcss-cssnext -o src/public/css/index.css src/client/index.css",
    "build-js": "browserify src/client/search/index.js src/client/profile/index.js src/client/home/index.js src/client/location/index.js -p [factor-bundle -o src/public/js/search.js -o src/public/js/profile.js -o src/public/js/home.js -o src/public/js/location.js] -o src/public/js/common.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "envlocalify"
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@f/serialize-form": "^1.0.0",
    "arrayify": "^1.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.15.0",
    "camelcase": "^2.1.0",
    "component-closest": "^1.0.0",
    "compression": "^1.6.1",
    "connect-pg-simple": "^3.1.0",
    "cookie-parser": "^1.4.0",
    "dotenv": "^1.2.0",
    "envify": "^3.4.0",
    "errorhandler": "^1.4.2",
    "express": "^4.13.3",
    "express-session": "^1.13.0",
    "failure": "^1.1.1",
    "form-serialize": "^0.7.0",
    "hbs": "^4.0.0",
    "hint": "github:kvnneff/hint",
    "hyperscript": "^1.4.7",
    "joi": "^7.0.1",
    "node-geocoder": "^3.5.1",
    "node-uuid": "^1.4.7",
    "nodemailer": "^2.1.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^4.4.3",
    "phone": "^1.0.5",
    "query-string": "^3.0.0",
    "sanitize-html": "^1.11.3",
    "session": "^0.1.0",
    "sort-by": "^1.1.1",
    "suitcss-base": "^1.0.1",
    "suitcss-components-button": "^5.0.0",
    "tel-format": "^1.0.7",
    "uglifyify": "^3.0.1",
    "url": "^0.11.0",
    "vanilla-masker": "github:bankfacil/vanilla-masker",
    "xhr": "^2.2.0"
  },
  "devDependencies": {
    "babelify": "^7.2.0",
    "component-clone": "^0.2.2",
    "deku-testutils": "^1.0.2",
    "envlocalify": "^1.1.1",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "factor-bundle": "^2.5.0",
    "mocha": "^2.3.4",
    "nock": "^5.2.1",
    "postcss-cli": "^2.5.1",
    "supertest": "^1.1.0",
    "zuul": "^3.8.0",
    "postcss-cssnext": "^2.3.0",
    "postcss-import": "^7.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kvnneff/egg.git"
